{
  "name": "etz",
  "version": "v1.1.21",
  "private": true,
  "scripts": {
    "dev": "node load-env.js && parcel serve  --open --host localhost --port 5140",
    "build": "rimraf build && parcel build src/pages/*.ejs src/**/*.html src/**/*.js src/**/*.css --out-dir build && mkdir -p build/src/ && cp -R src/* build/src/",
    "serve": "LOCAL_DEV=true node netlify/functions/server.js",
    "prepare": "husky install",
    "check:tsc": "tsc --noemit -t es5 ./src/**/*.ts",
    "check:lint": "eslint ./src --ext .js,.ts",
    "check:format": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json|css|html)\""
  },
  "lint-staged": {
    "./src/**/*.{ts,js}": [
      "eslint --fix"
    ],
    "./src/**/*.{js,ts,html,css,json}": [
      "prettier --write"
    ]
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "staticFiles": {
    "staticPath": "src/assets"
  },
  "source": [
    "src/pages/index.html",
    "src/pages/employers.html",
    "src/pages/legal.html",
    "src/pages/.well-known/change-password.html",
    "src/pages/assets.html",
    "src/pages/[coin].html"
  ],
  "devDependencies": {
    "@alienlebarge/stylelint-config": "^3.1.6",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@supabase/supabase-js": "^2.43.4",
    "@types/node": "^17.0.41",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "axios": "^1.7.2",
    "axios-retry": "^4.4.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.19.2",
    "https-proxy-agent": "^5.0.1",
    "husky": "^7.0.4",
    "lil-gui": "^0.16.1",
    "lint-staged": "^12.4.0",
    "openai": "^4.49.1",
    "parcel": "^2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-transformer-ejs": "^1.0.1",
    "postcss-bem-linter": "^4.0.1",
    "posthtml-include": "^1.7.3",
    "prettier": "2.6.2",
    "process": "^0.11.10",
    "proxy-agent": "^6.4.0",
    "puppeteer": "^22.10.0",
    "rimraf": "^3.0.2",
    "serverless-http": "^3.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "terser": "^5.31.1",
    "three": "^0.140.2",
    "typescript": "^4.6.3",
    "universalsmoothscroll": "^5.1.2",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "axios-retry": "^4.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "https-proxy-agent": "^5.0.1",
    "lil-gui": "^0.16.1",
    "openai": "^4.49.1",
    "parcel-bundler": "^1.12.5",
    "proxy-agent": "^6.4.0",
    "puppeteer": "^22.10.0",
    "serverless-http": "^3.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "three": "^0.140.2",
    "universalsmoothscroll": "^5.1.2",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  }
}
